#!/usr/bin/env php
<?php

declare(strict_types=1);

$secret = strtr(
    substr(
        base64_encode(random_bytes(24)),
        0,
        32
    ),
    '+/=',
    '_-.'
);

echo str_replace('<value>', addslashes($secret), trim(file_get_contents(
    __FILE__,
    false,
    null,
    __COMPILER_HALT_OFFSET__
))) . "\n";

__halt_compiler();
<?php

declare(strict_types=1);

return (function (): string {
    return "<value>";
})();
